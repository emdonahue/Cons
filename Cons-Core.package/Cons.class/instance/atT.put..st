element
atT: anIndex put: aVal
	"Binary tree at:put:, uses bitflag index generated by findT."
	self ifEmpty: [ SubscriptOutOfBounds signalFor: anIndex ].
	anIndex & 1 = 0 
		ifTrue: [ self car isCons 
			ifTrue: [ ^ (self car atT: anIndex >> 1 put: aVal) cons: self cdr ]
			ifFalse: 	[ ^ aVal cons: self cdr ]].
	self cdr isCons ifTrue: [ ^ self car cons: (self cdr atT: anIndex >> 1 put: aVal) ].
	^ self car cons: aVal.